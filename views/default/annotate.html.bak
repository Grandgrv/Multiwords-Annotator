  <script type="text/javascript"> 
    //for(i = 1; i < 2; i++)
      

        function funct(character) //for adding the clicked character to the textarea
    {
      //alert(character); 
      t = document.getElementById("text");
      t.value = t.value + character;
      //document.write(t.value);
    
    }
    
    function show_keyboard()
        {
          var s = document.getElementById("lang");
            try
            {
              for(i=0;i<=s.length;i++)
              {
              
                  m = document.getElementById("map"+i).style;
                  m.display = 'none';
             }
          }catch(er){}
            m = document.getElementById("map"+s.value).style;
        m.display = 'table';
        }
        
        function reset()  //to delete all written text in the form
    {
        
      t = document.getElementById("text");
      t.value = '';
    }
    
        function del() //to delete the last character on the form
    {
        
      t = document.getElementById("text");
      t.value = t.value.substring(0, (t.value.length*1 - 1));
    
    }
  </script>
