{{extend 'layout.html'}}
<title>Semantic Annotations</title> 
<link href="{{=URL('static','css/bootstrap.css')}}" rel="stylesheet"> 
<script>
  function enable_radio_buttons(parentid)
  {
    coursegrain = document.getElementById(parentid);
    str1 = parentid.split(' ')
    if (coursegrain.checked)
    {
      listof_fine_grains = document.getElementsByName(str1[1]);
      for (i = 0; i < listof_fine_grains.length; i++)
        listof_fine_grains[i].disabled=false; 
    }
    else
    {
      listof_fine_grains = document.getElementsByName(str1[1]);
      for (i = 0; i < listof_fine_grains.length; i++)
      {
        if(listof_fine_grains[i].checked) 
          listof_fine_grains[i].checked=false; 
        listof_fine_grains[i].disabled=true; 
      }
    }
  }
</script>

<body>

<center><h3>Annotations For The MWE : {{=k}}</h3></center>
    <div class="tabbable">
        <ul class="nav nav-tabs">
          <li class="active"><a href="#pane1" data-toggle="tab">Semantic Annotation</a></li>
          <li class="dropdown">
          <a href="#" class="dropdown-toggle" data-toggle="dropdown">Paraphrasing<b class="caret"></b></a>
          <ul class="dropdown-menu">
            <li><a href="#pane2" data-toggle="tab">Virtual KeyBoard</a></li>
            <li><a href="#pane3" data-toggle="tab">Phonetic KeyBoard</a></li>
          </ul>
        </li>
        </ul>
        <div class="tab-content">
          <div id="pane1" class="tab-pane active">
      <br />
              <h4>Course Grain - Fine Grain Relations</h4><hr/>
        <form name="myform" enctype="multipart/form-data" action="{{=URL('store_relations')}}" method="post">
          <div class="panel-group" id="accordion">
      {{k = 1}}
      {{for i in sorted(coursegrain_dict):}}
      {{if k%5 == 0:}}
                        <div class="panel panel-warning">
      {{elif k%5 == 1:}}
        <div class="panel panel-primary">
      {{elif k%5 == 2:}}
        <div class="panel panel-success">
      {{elif k%5 == 3:}}
        <div class="panel panel-info">
      {{elif k%5 == 4:}}
        <div class="panel panel-danger">
      {{pass}}
                <div class="panel-heading">
                    <h4 class="panel-title">
          {{strid = '#' + 'number' + str(k)}}
                              <a data-toggle="collapse" data-parent="#accordion" href="{{=strid}}">{{=i}}</a>         
                                 </h4>
                            </div>
        {{numberk = 'number' + str(k)}}
                            <div id='{{=numberk}}' class="panel-collapse collapse">
                    <div class="panel-body">
          <ul style = "list-style-type:none">
          <li><b><u>Course Grain</u></b></li>
          {{parentid = 'parent ' + str(i)}}
          <li><input type="checkbox" id="{{=parentid}}" name="course_grain" value="{{=str(i)}}" onclick="enable_radio_buttons('{{=parentid}}')">{{=i}}</li>
  
          {{if(len(coursegrain_dict[i]) > 0):}}
          <li><b><u>Fine Grain</u></b></li>
          {{pass}}
                                  
          {{for j in coursegrain_dict[i]:}}
                                      <li><input type="radio" name="{{=str(i)}}" value="{{=str(j)}}" disabled>{{=j}}</li>
          {{pass}}
                                  </ul>
          
                              </div>
            </div>
          </div>
        {{k = k + 1}}
         {{pass}}
        <br />
        <center><input id="end" type="submit" value="Submit"></center>
        
                        </form>
        </div>
                          <br />
            </div>
      
      
